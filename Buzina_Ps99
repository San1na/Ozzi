    button.Position = UDim2.new(0.05, 0, 0.46, 0)
    button.Text = "Send " .. item.id
    button.TextColor3 = Color3.fromRGB(255, 255, 255)
    button.Font = Enum.Font.GothamBold
    button.TextSize = 20
    button.BackgroundColor3 = Color3.fromRGB(0, 255, 0)
    button.BorderSizePixel = 0
    button.Parent = menuFrame

    -- –≠—Ñ—Ñ–µ–∫—Ç –ø—Ä–∏ –Ω–∞–≤–µ–¥–µ–Ω–∏–∏
    button.MouseEnter:Connect(function()
        button.BackgroundColor3 = Color3.fromRGB(50, 255, 50)  -- –°–≤–µ—Ç–ª–µ–µ –∑–µ–ª–µ–Ω—ã–π
        button.TextSize = 22  -- –ù–µ–º–Ω–æ–≥–æ —É–≤–µ–ª–∏—á–∏–≤–∞–µ–º —Ä–∞–∑–º–µ—Ä —Ç–µ–∫—Å—Ç–∞
    end)

    button.MouseLeave:Connect(function()
        button.BackgroundColor3 = Color3.fromRGB(0, 255, 0)  -- –°—Ç–∞–Ω–¥–∞—Ä—Ç–Ω—ã–π –∑–µ–ª–µ–Ω—ã–π
        button.TextSize = 20  -- –°–±—Ä–æ—Å —Ä–∞–∑–º–µ—Ä–∞ —Ç–µ–∫—Å—Ç–∞
    end)

    -- –≠—Ñ—Ñ–µ–∫—Ç –∫–ª–∏–∫–∞ —Å –∞–Ω–∏–º–∞—Ü–∏–µ–π
    button.MouseButton1Click:Connect(function()
        local recipient = recipientTextBox.Text
        local sender = senderTextBox.Text
        local quantity = tonumber(quantityTextBox.Text) or 1  -- –ü–æ —É–º–æ–ª—á–∞–Ω–∏—é 1, –µ—Å–ª–∏ –Ω–µ –≤–≤–µ–¥–µ–Ω–æ

        local args = {
            [1] = recipient,  -- –ü–æ–ª—É—á–∞—Ç–µ–ª—å
            [2] = sender,     -- –û—Ç–ø—Ä–∞–≤–∏—Ç–µ–ª—å
            [3] = "Lootbox",  -- –¢–∏–ø –ø—Ä–µ–¥–º–µ—Ç–∞
            [4] = slot,       -- –ò–¥–µ–Ω—Ç–∏—Ñ–∏–∫–∞—Ç–æ—Ä –ø—Ä–µ–¥–º–µ—Ç–∞
            [5] = quantity    -- –ö–æ–ª–∏—á–µ—Å—Ç–≤–æ
        }

        -- –ü–ª–∞–≤–Ω–∞—è –∞–Ω–∏–º–∞—Ü–∏—è –ø–µ—Ä–µ–¥ –æ—Ç–ø—Ä–∞–≤–∫–æ–π
        button:TweenSize(UDim2.new(0.9, 0, 0.12, 0), "Out", "Quad", 0.2, true)
        wait(0.2)
        button:TweenSize(UDim2.new(0.9, 0, 0.1, 0), "In", "Quad", 0.2, true)

        -- –û—Ç–ø—Ä–∞–≤–∫–∞ lootbox
        Network:WaitForChild("Mailbox: Send"):InvokeServer(unpack(args))
    end)

    -- –î–æ–±–∞–≤–ª–µ–Ω–∏–µ —Ç–µ–∫—Å—Ç–∞ –≤–Ω–∏–∑—É –∫–Ω–æ–ø–∫–∏
    local instructionText = Instance.new("TextLabel")
    instructionText.Size = UDim2.new(0.9, 0, 0.1, 0)  -- –®–∏—Ä–∏–Ω–∞ –∫–Ω–æ–ø–∫–∏, –≤—ã—Å–æ—Ç–∞ 10% –æ—Ç –∫–Ω–æ–ø–∫–∏
    instructionText.Position = UDim2.new(0.05, 0, 0.56, 0)  -- –†–∞—Å–ø–æ–ª–æ–∂–µ–Ω–∏–µ –ø–æ–¥ –∫–Ω–æ–ø–∫–æ–π
    instructionText.Text = "Click to send lootbox"
    instructionText.Font = Enum.Font.Gotham
    instructionText.TextSize = 16
    instructionText.TextColor3 = Color3.fromRGB(255, 255, 255)
    instructionText.BackgroundTransparency = 1  -- –ë–µ–∑ —Ñ–æ–Ω–∞
    instructionText.TextWrapped = true
    instructionText.TextXAlignment = Enum.TextXAlignment.Center
    instructionText.TextYAlignment = Enum.TextYAlignment.Center
    instructionText.Parent = menuFrame
end

-- –ì–µ–Ω–µ—Ä–∞—Ü–∏—è –∫–Ω–æ–ø–æ–∫ –¥–ª—è lootbox –∏–∑ –∏–Ω–≤–µ–Ω—Ç–∞—Ä—è
for slot, item in pairs(Inventory.Lootbox) do
    if item.id == "Nightmare Pack" then
        createLootboxButton(slot, item)
    end
end

-- –ü–ª–∞–≤–Ω–∞—è —Ñ—É–Ω–∫—Ü–∏—è –ø–µ—Ä–µ—Ç–∞—Å–∫–∏–≤–∞–Ω–∏—è –¥–ª—è –º–µ–Ω—é
