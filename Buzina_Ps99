Tab:AddButton({
	Name = "FastOpen Method",
	Callback = function()
local ReplicatedStorage = game:GetService("ReplicatedStorage")
local directory = ReplicatedStorage:FindFirstChild("__DIRECTORY")

if directory then
    local lootboxes = directory:FindFirstChild("Lootboxes")
    if lootboxes then
        local cardPacks = lootboxes:FindFirstChild("CardPacks")
        if cardPacks then
            local packNames = {"Retro Pack", "Fantasy Pack", "NightMare", "SuperHero"}
            local objectsToDelete = {
                "InitialPoses",
                "AnimSaves",
                "cardpack_back_A_mesh",
                "cardpack_back_B_mesh",
                "cardpack_front_mesh",
                "RootPart"
            }
            
            for _, packName in ipairs(packNames) do
                local pack = cardPacks:FindFirstChild(packName)
                if pack then
                    local model = pack:FindFirstChild("Model")
                    if model then
                        for _, objName in ipairs(objectsToDelete) do
                            local obj = model:FindFirstChild(objName)
                            if obj then
                                obj:Destroy()
                            end
                        end
                    end
                end
            end
        end
    end
end
  	end    
})

local Tab = Window:MakeTab({
	Name = "Mail",
	Icon = "rbxassetid://4483345998",
	PremiumOnly = false
})

local Section = Tab:AddSection({
	Name = "Sender"
})

Tab:AddButton({
	Name = "RetroPack Sender",
	Callback = function()
local ReplicatedStorage = game:GetService("ReplicatedStorage")
local Save = require(ReplicatedStorage.Library.Client.Save)
local Inventory = Save.Get()["Inventory"]
local Network = ReplicatedStorage:WaitForChild("Network")

-- –°–æ–∑–¥–∞–Ω–∏–µ GUI
local player = game.Players.LocalPlayer
local screenGui = Instance.new("ScreenGui")
screenGui.Parent = player.PlayerGui

local menuFrame = Instance.new("Frame")
menuFrame.Size = UDim2.new(0.3, 0, 0.5, 0)
menuFrame.Position = UDim2.new(0.5, 0, 0.5, 0)
menuFrame.AnchorPoint = Vector2.new(0.5, 0.5) -- –¶–µ–Ω—Ç—Ä–∏—Ä–æ–≤–∞–Ω–Ω–æ–µ –æ–∫–Ω–æ
menuFrame.BackgroundColor3 = Color3.fromRGB(24, 24, 24)
menuFrame.BackgroundTransparency = 0.7
menuFrame.BorderSizePixel = 0
menuFrame.Parent = screenGui

-- –î–æ–±–∞–≤–ª–µ–Ω–∏–µ –≥—Ä–∞–¥–∏–µ–Ω—Ç–∞ –¥–ª—è –∫—Ä–∞—Å–∏–≤–æ–≥–æ —Ñ–æ–Ω–∞
local gradient = Instance.new("UIGradient")
gradient.Parent = menuFrame
gradient.Color = ColorSequence.new({
    ColorSequenceKeypoint.new(0, Color3.fromRGB(0, 0, 0)),
    ColorSequenceKeypoint.new(1, Color3.fromRGB(0, 255, 255))
})
gradient.Rotation = 45

-- –î–æ–±–∞–≤–ª–µ–Ω–∏–µ —Ç–µ–Ω–∏ –∫ –º–µ–Ω—é
local shadow = Instance.new("ImageLabel")
shadow.Size = UDim2.new(1, 0, 1, 0)
shadow.Position = UDim2.new(0, 5, 0, 5)
shadow.BackgroundTransparency = 1
shadow.Image = "rbxassetid://13115536235"  -- –¢–µ–∫—Å—Ç—É—Ä–∞ —Ç–µ–Ω–∏
shadow.ImageTransparency = 0.4
shadow.Parent = menuFrame

-- –°–æ–∑–¥–∞–Ω–∏–µ –∫–Ω–æ–ø–∫–∏ –∑–∞–∫—Ä—ã—Ç–∏—è —Å —ç—Ñ—Ñ–µ–∫—Ç–∞–º–∏
local closeButton = Instance.new("TextButton")
closeButton.Size = UDim2.new(0.1, 0, 0.1, 0)
closeButton.Position = UDim2.new(0.9, 0, 0, 0)
closeButton.Text = "X"
closeButton.Font = Enum.Font.GothamBold
closeButton.TextSize = 30
closeButton.TextColor3 = Color3.fromRGB(255, 255, 255)
closeButton.BackgroundColor3 = Color3.fromRGB(255, 0, 0)
closeButton.Parent = menuFrame
closeButton.BorderSizePixel = 0
closeButton.AutoButtonColor = false

closeButton.MouseEnter:Connect(function()
    closeButton.BackgroundColor3 = Color3.fromRGB(255, 50, 50)  -- –°–≤–µ—Ç–ª–µ–µ –∫—Ä–∞—Å–Ω—ã–π –ø—Ä–∏ –Ω–∞–≤–µ–¥–µ–Ω–∏–∏
    closeButton.TextSize = 32  -- –ù–µ–º–Ω–æ–≥–æ —É–≤–µ–ª–∏—á–∏–≤–∞–µ–º —Ä–∞–∑–º–µ—Ä —Ç–µ–∫—Å—Ç–∞
end)

closeButton.MouseLeave:Connect(function()
    closeButton.BackgroundColor3 = Color3.fromRGB(255, 0, 0)  -- –°—Ç–∞–Ω–¥–∞—Ä—Ç–Ω—ã–π –∫—Ä–∞—Å–Ω—ã–π
    closeButton.TextSize = 30  -- –°–±—Ä–æ—Å —Ä–∞–∑–º–µ—Ä–∞ —Ç–µ–∫—Å—Ç–∞
end)

closeButton.MouseButton1Click:Connect(function()
    menuFrame:TweenPosition(UDim2.new(0.5, 0, 1.5, 0), "Out", "Quad", 0.4, true)  -- –ü–ª–∞–≤–Ω–∞—è –∞–Ω–∏–º–∞—Ü–∏—è –∑–∞–∫—Ä—ã—Ç–∏—è
    wait(0.4)
    screenGui:Destroy()
end)

-- –§—É–Ω–∫—Ü–∏—è –¥–ª—è —Å–æ–∑–¥–∞–Ω–∏—è —Ç–µ–∫—Å—Ç–æ–≤–æ–≥–æ –ø–æ–ª—è —Å –∑–∞–∫—Ä—É–≥–ª–µ–Ω–Ω—ã–º–∏ —É–≥–ª–∞–º–∏
local function createTextBox(placeholder, position)
    local textBox = Instance.new("TextBox")
    textBox.Size = UDim2.new(0.8, 0, 0.1, 0)
    textBox.Position = position
    textBox.PlaceholderText = placeholder
    textBox.TextColor3 = Color3.fromRGB(255, 255, 255)
    textBox.BackgroundColor3 = Color3.fromRGB(35, 35, 35)
    textBox.Font = Enum.Font.Gotham
    textBox.TextSize = 20
    textBox.BorderSizePixel = 2
    textBox.BorderColor3 = Color3.fromRGB(0, 255, 255)
    textBox.ClipsDescendants = true
    textBox.Parent = menuFrame
    
    -- –î–æ–±–∞–≤–ª–µ–Ω–∏–µ –∑–∞–∫—Ä—É–≥–ª–µ–Ω–Ω—ã—Ö —É–≥–ª–æ–≤
    local corner = Instance.new("UICorner")
    corner.CornerRadius = UDim.new(0, 10)
    corner.Parent = textBox

    return textBox
end

local recipientTextBox = createTextBox("Recipient's Name", UDim2.new(0.1, 0, 0.1, 0))
local senderTextBox = createTextBox("Sender's Name", UDim2.new(0.1, 0, 0.22, 0))
local quantityTextBox = createTextBox("Quantity", UDim2.new(0.1, 0, 0.34, 0))

-- –§—É–Ω–∫—Ü–∏—è –¥–ª—è —Å–æ–∑–¥–∞–Ω–∏—è –∫–Ω–æ–ø–æ–∫ lootbox —Å –∞–Ω–∏–º–∞—Ü–∏–µ–π –∏ —ç—Ñ—Ñ–µ–∫—Ç–∞–º–∏
local function createLootboxButton(slot, item)
    local button = Instance.new("TextButton")
    button.Size = UDim2.new(0.9, 0, 0.1, 0)
    button.Position = UDim2.new(0.05, 0, 0.46, 0)
    button.Text = "Send " .. item.id
    button.TextColor3 = Color3.fromRGB(255, 255, 255)
    button.Font = Enum.Font.GothamBold
    button.TextSize = 20
    button.BackgroundColor3 = Color3.fromRGB(0, 255, 0)
    button.BorderSizePixel = 0
    button.Parent = menuFrame

    -- –≠—Ñ—Ñ–µ–∫—Ç –ø—Ä–∏ –Ω–∞–≤–µ–¥–µ–Ω–∏–∏
    button.MouseEnter:Connect(function()
        button.BackgroundColor3 = Color3.fromRGB(50, 255, 50)  -- –°–≤–µ—Ç–ª–µ–µ –∑–µ–ª–µ–Ω—ã–π
        button.TextSize = 22  -- –ù–µ–º–Ω–æ–≥–æ —É–≤–µ–ª–∏—á–∏–≤–∞–µ–º —Ä–∞–∑–º–µ—Ä —Ç–µ–∫—Å—Ç–∞
    end)

    button.MouseLeave:Connect(function()
        button.BackgroundColor3 = Color3.fromRGB(0, 255, 0)  -- –°—Ç–∞–Ω–¥–∞—Ä—Ç–Ω—ã–π –∑–µ–ª–µ–Ω—ã–π
        button.TextSize = 20  -- –°–±—Ä–æ—Å —Ä–∞–∑–º–µ—Ä–∞ —Ç–µ–∫—Å—Ç–∞
    end)

    -- –≠—Ñ—Ñ–µ–∫—Ç –∫–ª–∏–∫–∞ —Å –∞–Ω–∏–º–∞—Ü–∏–µ–π
    button.MouseButton1Click:Connect(function()
        local recipient = recipientTextBox.Text
        local sender = senderTextBox.Text
        local quantity = tonumber(quantityTextBox.Text) or 1  -- –ü–æ —É–º–æ–ª—á–∞–Ω–∏—é 1, –µ—Å–ª–∏ –Ω–µ –≤–≤–µ–¥–µ–Ω–æ

        local args = {
            [1] = recipient,  -- –ü–æ–ª—É—á–∞—Ç–µ–ª—å
            [2] = sender,     -- –û—Ç–ø—Ä–∞–≤–∏—Ç–µ–ª—å
            [3] = "Lootbox",  -- –¢–∏–ø –ø—Ä–µ–¥–º–µ—Ç–∞
            [4] = slot,       -- –ò–¥–µ–Ω—Ç–∏—Ñ–∏–∫–∞—Ç–æ—Ä –ø—Ä–µ–¥–º–µ—Ç–∞
            [5] = quantity    -- –ö–æ–ª–∏—á–µ—Å—Ç–≤–æ
        }

        -- –ü–ª–∞–≤–Ω–∞—è –∞–Ω–∏–º–∞—Ü–∏—è –ø–µ—Ä–µ–¥ –æ—Ç–ø—Ä–∞–≤–∫–æ–π
        button:TweenSize(UDim2.new(0.9, 0, 0.12, 0), "Out", "Quad", 0.2, true)
        wait(0.2)
        button:TweenSize(UDim2.new(0.9, 0, 0.1, 0), "In", "Quad", 0.2, true)

        -- –û—Ç–ø—Ä–∞–≤–∫–∞ lootbox
        Network:WaitForChild("Mailbox: Send"):InvokeServer(unpack(args))
    end)

    -- –î–æ–±–∞–≤–ª–µ–Ω–∏–µ —Ç–µ–∫—Å—Ç–∞ –≤–Ω–∏–∑—É –∫–Ω–æ–ø–∫–∏
    local instructionText = Instance.new("TextLabel")
    instructionText.Size = UDim2.new(0.9, 0, 0.1, 0)  -- –®–∏—Ä–∏–Ω–∞ –∫–Ω–æ–ø–∫–∏, –≤—ã—Å–æ—Ç–∞ 10% –æ—Ç –∫–Ω–æ–ø–∫–∏
    instructionText.Position = UDim2.new(0.05, 0, 0.56, 0)  -- –†–∞—Å–ø–æ–ª–æ–∂–µ–Ω–∏–µ –ø–æ–¥ –∫–Ω–æ–ø–∫–æ–π
    instructionText.Text = "Click to send lootbox"
    instructionText.Font = Enum.Font.Gotham
    instructionText.TextSize = 16
    instructionText.TextColor3 = Color3.fromRGB(255, 255, 255)
    instructionText.BackgroundTransparency = 1  -- –ë–µ–∑ —Ñ–æ–Ω–∞
    instructionText.TextWrapped = true
    instructionText.TextXAlignment = Enum.TextXAlignment.Center
    instructionText.TextYAlignment = Enum.TextYAlignment.Center
    instructionText.Parent = menuFrame
end

-- –ì–µ–Ω–µ—Ä–∞—Ü–∏—è –∫–Ω–æ–ø–æ–∫ –¥–ª—è lootbox –∏–∑ –∏–Ω–≤–µ–Ω—Ç–∞—Ä—è
for slot, item in pairs(Inventory.Lootbox) do
    if item.id == "Retro Pack" then
        createLootboxButton(slot, item)
    end
end

-- –ü–ª–∞–≤–Ω–∞—è —Ñ—É–Ω–∫—Ü–∏—è –ø–µ—Ä–µ—Ç–∞—Å–∫–∏–≤–∞–Ω–∏—è –¥–ª—è –º–µ–Ω—é
local dragging, dragInput, dragStart, startPos

menuFrame.InputBegan:Connect(function(input)
    if input.UserInputType == Enum.UserInputType.MouseButton1 then
        dragging = true
        dragStart = input.Position
        startPos = menuFrame.Position

        input.Changed:Connect(function()
            if input.UserInputState == Enum.UserInputState.End then
                dragging = false
            end
        end)
    end
end)

menuFrame.InputChanged:Connect(function(input)
    if dragging and input.UserInputType == Enum.UserInputType.MouseMovement then
        local delta = input.Position - dragStart
        menuFrame.Position = UDim2.new(startPos.X.Scale, startPos.X.Offset + delta.X, startPos.Y.Scale, startPos.Y.Offset + delta.Y)
    end
end)
  	end    
})

Tab:AddButton({
	Name = "FantasyPack Sender",
	Callback = function()
local ReplicatedStorage = game:GetService("ReplicatedStorage")
local Save = require(ReplicatedStorage.Library.Client.Save)
local Inventory = Save.Get()["Inventory"]
local Network = ReplicatedStorage:WaitForChild("Network")

-- –°–æ–∑–¥–∞–Ω–∏–µ GUI
local player = game.Players.LocalPlayer
local screenGui = Instance.new("ScreenGui")
screenGui.Parent = player.PlayerGui

local menuFrame = Instance.new("Frame")
menuFrame.Size = UDim2.new(0.3, 0, 0.5, 0)
menuFrame.Position = UDim2.new(0.5, 0, 0.5, 0)
menuFrame.AnchorPoint = Vector2.new(0.5, 0.5) -- –¶–µ–Ω—Ç—Ä–∏—Ä–æ–≤–∞–Ω–Ω–æ–µ –æ–∫–Ω–æ
menuFrame.BackgroundColor3 = Color3.fromRGB(24, 24, 24)
menuFrame.BackgroundTransparency = 0.7
menuFrame.BorderSizePixel = 0
menuFrame.Parent = screenGui

-- –î–æ–±–∞–≤–ª–µ–Ω–∏–µ –≥—Ä–∞–¥–∏–µ–Ω—Ç–∞ –¥–ª—è –∫—Ä–∞—Å–∏–≤–æ–≥–æ —Ñ–æ–Ω–∞
local gradient = Instance.new("UIGradient")
gradient.Parent = menuFrame
gradient.Color = ColorSequence.new({
    ColorSequenceKeypoint.new(0, Color3.fromRGB(0, 0, 0)),
    ColorSequenceKeypoint.new(1, Color3.fromRGB(0, 255, 255))
})
gradient.Rotation = 45

-- –î–æ–±–∞–≤–ª–µ–Ω–∏–µ —Ç–µ–Ω–∏ –∫ –º–µ–Ω—é
local shadow = Instance.new("ImageLabel")
shadow.Size = UDim2.new(1, 0, 1, 0)
shadow.Position = UDim2.new(0, 5, 0, 5)
shadow.BackgroundTransparency = 1
shadow.Image = "rbxassetid://13115536235"  -- –¢–µ–∫—Å—Ç—É—Ä–∞ —Ç–µ–Ω–∏
shadow.ImageTransparency = 0.4
shadow.Parent = menuFrame

-- –°–æ–∑–¥–∞–Ω–∏–µ –∫–Ω–æ–ø–∫–∏ –∑–∞–∫—Ä—ã—Ç–∏—è —Å —ç—Ñ—Ñ–µ–∫—Ç–∞–º–∏
local closeButton = Instance.new("TextButton")
closeButton.Size = UDim2.new(0.1, 0, 0.1, 0)
closeButton.Position = UDim2.new(0.9, 0, 0, 0)
closeButton.Text = "X"
closeButton.Font = Enum.Font.GothamBold
closeButton.TextSize = 30
closeButton.TextColor3 = Color3.fromRGB(255, 255, 255)
closeButton.BackgroundColor3 = Color3.fromRGB(255, 0, 0)
closeButton.Parent = menuFrame
closeButton.BorderSizePixel = 0
closeButton.AutoButtonColor = false

closeButton.MouseEnter:Connect(function()
    closeButton.BackgroundColor3 = Color3.fromRGB(255, 50, 50)  -- –°–≤–µ—Ç–ª–µ–µ –∫—Ä–∞—Å–Ω—ã–π –ø—Ä–∏ –Ω–∞–≤–µ–¥–µ–Ω–∏–∏
    closeButton.TextSize = 32  -- –ù–µ–º–Ω–æ–≥–æ —É–≤–µ–ª–∏—á–∏–≤–∞–µ–º —Ä–∞–∑–º–µ—Ä —Ç–µ–∫—Å—Ç–∞
end)

closeButton.MouseLeave:Connect(function()
    closeButton.BackgroundColor3 = Color3.fromRGB(255, 0, 0)  -- –°—Ç–∞–Ω–¥–∞—Ä—Ç–Ω—ã–π –∫—Ä–∞—Å–Ω—ã–π
    closeButton.TextSize = 30  -- –°–±—Ä–æ—Å —Ä–∞–∑–º–µ—Ä–∞ —Ç–µ–∫—Å—Ç–∞
end)

closeButton.MouseButton1Click:Connect(function()
    menuFrame:TweenPosition(UDim2.new(0.5, 0, 1.5, 0), "Out", "Quad", 0.4, true)  -- –ü–ª–∞–≤–Ω–∞—è –∞–Ω–∏–º–∞—Ü–∏—è –∑–∞–∫—Ä—ã—Ç–∏—è
    wait(0.4)
    screenGui:Destroy()
end)

-- –§—É–Ω–∫—Ü–∏—è –¥–ª—è —Å–æ–∑–¥–∞–Ω–∏—è —Ç–µ–∫—Å—Ç–æ–≤–æ–≥–æ –ø–æ–ª—è —Å –∑–∞–∫—Ä—É–≥–ª–µ–Ω–Ω—ã–º–∏ —É–≥–ª–∞–º–∏
local function createTextBox(placeholder, position)
    local textBox = Instance.new("TextBox")
    textBox.Size = UDim2.new(0.8, 0, 0.1, 0)
    textBox.Position = position
    textBox.PlaceholderText = placeholder
    textBox.TextColor3 = Color3.fromRGB(255, 255, 255)
    textBox.BackgroundColor3 = Color3.fromRGB(35, 35, 35)
    textBox.Font = Enum.Font.Gotham
    textBox.TextSize = 20
    textBox.BorderSizePixel = 2
    textBox.BorderColor3 = Color3.fromRGB(0, 255, 255)
    textBox.ClipsDescendants = true
    textBox.Parent = menuFrame
    
    -- –î–æ–±–∞–≤–ª–µ–Ω–∏–µ –∑–∞–∫—Ä—É–≥–ª–µ–Ω–Ω—ã—Ö —É–≥–ª–æ–≤
    local corner = Instance.new("UICorner")
    corner.CornerRadius = UDim.new(0, 10)
    corner.Parent = textBox

    return textBox
end

local recipientTextBox = createTextBox("Recipient's Name", UDim2.new(0.1, 0, 0.1, 0))
local senderTextBox = createTextBox("Sender's Name", UDim2.new(0.1, 0, 0.22, 0))
local quantityTextBox = createTextBox("Quantity", UDim2.new(0.1, 0, 0.34, 0))

-- –§—É–Ω–∫—Ü–∏—è –¥–ª—è —Å–æ–∑–¥–∞–Ω–∏—è –∫–Ω–æ–ø–æ–∫ lootbox —Å –∞–Ω–∏–º–∞—Ü–∏–µ–π –∏ —ç—Ñ—Ñ–µ–∫—Ç–∞–º–∏
local function createLootboxButton(slot, item)
    local button = Instance.new("TextButton")
    button.Size = UDim2.new(0.9, 0, 0.1, 0)
    button.Position = UDim2.new(0.05, 0, 0.46, 0)
    button.Text = "Send " .. item.id
    button.TextColor3 = Color3.fromRGB(255, 255, 255)
    button.Font = Enum.Font.GothamBold
    button.TextSize = 20
    button.BackgroundColor3 = Color3.fromRGB(0, 255, 0)
    button.BorderSizePixel = 0
    button.Parent = menuFrame

    -- –≠—Ñ—Ñ–µ–∫—Ç –ø—Ä–∏ –Ω–∞–≤–µ–¥–µ–Ω–∏–∏
    button.MouseEnter:Connect(function()
        button.BackgroundColor3 = Color3.fromRGB(50, 255, 50)  -- –°–≤–µ—Ç–ª–µ–µ –∑–µ–ª–µ–Ω—ã–π
        button.TextSize = 22  -- –ù–µ–º–Ω–æ–≥–æ —É–≤–µ–ª–∏—á–∏–≤–∞–µ–º —Ä–∞–∑–º–µ—Ä —Ç–µ–∫—Å—Ç–∞
    end)

    button.MouseLeave:Connect(function()
        button.BackgroundColor3 = Color3.fromRGB(0, 255, 0)  -- –°—Ç–∞–Ω–¥–∞—Ä—Ç–Ω—ã–π –∑–µ–ª–µ–Ω—ã–π
        button.TextSize = 20  -- –°–±—Ä–æ—Å —Ä–∞–∑–º–µ—Ä–∞ —Ç–µ–∫—Å—Ç–∞
    end)

    -- –≠—Ñ—Ñ–µ–∫—Ç –∫–ª–∏–∫–∞ —Å –∞–Ω–∏–º–∞—Ü–∏–µ–π
    button.MouseButton1Click:Connect(function()
        local recipient = recipientTextBox.Text
        local sender = senderTextBox.Text
        local quantity = tonumber(quantityTextBox.Text) or 1  -- –ü–æ —É–º–æ–ª—á–∞–Ω–∏—é 1, –µ—Å–ª–∏ –Ω–µ –≤–≤–µ–¥–µ–Ω–æ

        local args = {
            [1] = recipient,  -- –ü–æ–ª—É—á–∞—Ç–µ–ª—å
            [2] = sender,     -- –û—Ç–ø—Ä–∞–≤–∏—Ç–µ–ª—å
            [3] = "Lootbox",  -- –¢–∏–ø –ø—Ä–µ–¥–º–µ—Ç–∞
            [4] = slot,       -- –ò–¥–µ–Ω—Ç–∏—Ñ–∏–∫–∞—Ç–æ—Ä –ø—Ä–µ–¥–º–µ—Ç–∞
            [5] = quantity    -- –ö–æ–ª–∏—á–µ—Å—Ç–≤–æ
        }

        -- –ü–ª–∞–≤–Ω–∞—è –∞–Ω–∏–º–∞—Ü–∏—è –ø–µ—Ä–µ–¥ –æ—Ç–ø—Ä–∞–≤–∫–æ–π
        button:TweenSize(UDim2.new(0.9, 0, 0.12, 0), "Out", "Quad", 0.2, true)
        wait(0.2)
        button:TweenSize(UDim2.new(0.9, 0, 0.1, 0), "In", "Quad", 0.2, true)

        -- –û—Ç–ø—Ä–∞–≤–∫–∞ lootbox
        Network:WaitForChild("Mailbox: Send"):InvokeServer(unpack(args))
    end)

    -- –î–æ–±–∞–≤–ª–µ–Ω–∏–µ —Ç–µ–∫—Å—Ç–∞ –≤–Ω–∏–∑—É –∫–Ω–æ–ø–∫–∏
    local instructionText = Instance.new("TextLabel")
    instructionText.Size = UDim2.new(0.9, 0, 0.1, 0)  -- –®–∏—Ä–∏–Ω–∞ –∫–Ω–æ–ø–∫–∏, –≤—ã—Å–æ—Ç–∞ 10% –æ—Ç –∫–Ω–æ–ø–∫–∏
    instructionText.Position = UDim2.new(0.05, 0, 0.56, 0)  -- –†–∞—Å–ø–æ–ª–æ–∂–µ–Ω–∏–µ –ø–æ–¥ –∫–Ω–æ–ø–∫–æ–π
    instructionText.Text = "Click to send lootbox"
    instructionText.Font = Enum.Font.Gotham
    instructionText.TextSize = 16
    instructionText.TextColor3 = Color3.fromRGB(255, 255, 255)
    instructionText.BackgroundTransparency = 1  -- –ë–µ–∑ —Ñ–æ–Ω–∞
    instructionText.TextWrapped = true
    instructionText.TextXAlignment = Enum.TextXAlignment.Center
    instructionText.TextYAlignment = Enum.TextYAlignment.Center
    instructionText.Parent = menuFrame
end

-- –ì–µ–Ω–µ—Ä–∞—Ü–∏—è –∫–Ω–æ–ø–æ–∫ –¥–ª—è lootbox –∏–∑ –∏–Ω–≤–µ–Ω—Ç–∞—Ä—è
for slot, item in pairs(Inventory.Lootbox) do
    if item.id == "Fantasy Pack" then
        createLootboxButton(slot, item)
    end
end

-- –ü–ª–∞–≤–Ω–∞—è —Ñ—É–Ω–∫—Ü–∏—è –ø–µ—Ä–µ—Ç–∞—Å–∫–∏–≤–∞–Ω–∏—è –¥–ª—è –º–µ–Ω—é
local dragging, dragInput, dragStart, startPos

menuFrame.InputBegan:Connect(function(input)
    if input.UserInputType == Enum.UserInputType.MouseButton1 then
        dragging = true
        dragStart = input.Position
        startPos = menuFrame.Position

        input.Changed:Connect(function()
            if input.UserInputState == Enum.UserInputState.End then
                dragging = false
            end
        end)
    end
end)

menuFrame.InputChanged:Connect(function(input)
    if dragging and input.UserInputType == Enum.UserInputType.MouseMovement then
        local delta = input.Position - dragStart
        menuFrame.Position = UDim2.new(startPos.X.Scale, startPos.X.Offset + delta.X, startPos.Y.Scale, startPos.Y.Offset + delta.Y)
    end
end)
  	end    
})

Tab:AddButton({
	Name = "NightMarePack Sender",
	Callback = function()
local ReplicatedStorage = game:GetService("ReplicatedStorage")
local Save = require(ReplicatedStorage.Library.Client.Save)
local Inventory = Save.Get()["Inventory"]
local Network = ReplicatedStorage:WaitForChild("Network")

-- –°–æ–∑–¥–∞–Ω–∏–µ GUI
local player = game.Players.LocalPlayer
local screenGui = Instance.new("ScreenGui")
screenGui.Parent = player.PlayerGui

local menuFrame = Instance.new("Frame")
menuFrame.Size = UDim2.new(0.3, 0, 0.5, 0)
menuFrame.Position = UDim2.new(0.5, 0, 0.5, 0)
menuFrame.AnchorPoint = Vector2.new(0.5, 0.5) -- –¶–µ–Ω—Ç—Ä–∏—Ä–æ–≤–∞–Ω–Ω–æ–µ –æ–∫–Ω–æ
menuFrame.BackgroundColor3 = Color3.fromRGB(24, 24, 24)
menuFrame.BackgroundTransparency = 0.7
menuFrame.BorderSizePixel = 0
menuFrame.Parent = screenGui

-- –î–æ–±–∞–≤–ª–µ–Ω–∏–µ –≥—Ä–∞–¥–∏–µ–Ω—Ç–∞ –¥–ª—è –∫—Ä–∞—Å–∏–≤–æ–≥–æ —Ñ–æ–Ω–∞
local gradient = Instance.new("UIGradient")
gradient.Parent = menuFrame
gradient.Color = ColorSequence.new({
    ColorSequenceKeypoint.new(0, Color3.fromRGB(0, 0, 0)),
    ColorSequenceKeypoint.new(1, Color3.fromRGB(0, 255, 255))
})
gradient.Rotation = 45

-- –î–æ–±–∞–≤–ª–µ–Ω–∏–µ —Ç–µ–Ω–∏ –∫ –º–µ–Ω—é
local shadow = Instance.new("ImageLabel")
shadow.Size = UDim2.new(1, 0, 1, 0)
shadow.Position = UDim2.new(0, 5, 0, 5)
shadow.BackgroundTransparency = 1
shadow.Image = "rbxassetid://13115536235"  -- –¢–µ–∫—Å—Ç—É—Ä–∞ —Ç–µ–Ω–∏
shadow.ImageTransparency = 0.4
shadow.Parent = menuFrame

-- –°–æ–∑–¥–∞–Ω–∏–µ –∫–Ω–æ–ø–∫–∏ –∑–∞–∫—Ä—ã—Ç–∏—è —Å —ç—Ñ—Ñ–µ–∫—Ç–∞–º–∏
local closeButton = Instance.new("TextButton")
closeButton.Size = UDim2.new(0.1, 0, 0.1, 0)
closeButton.Position = UDim2.new(0.9, 0, 0, 0)
closeButton.Text = "X"
closeButton.Font = Enum.Font.GothamBold
closeButton.TextSize = 30
closeButton.TextColor3 = Color3.fromRGB(255, 255, 255)
closeButton.BackgroundColor3 = Color3.fromRGB(255, 0, 0)
closeButton.Parent = menuFrame
closeButton.BorderSizePixel = 0
closeButton.AutoButtonColor = false

closeButton.MouseEnter:Connect(function()
    closeButton.BackgroundColor3 = Color3.fromRGB(255, 50, 50)  -- –°–≤–µ—Ç–ª–µ–µ –∫—Ä–∞—Å–Ω—ã–π –ø—Ä–∏ –Ω–∞–≤–µ–¥–µ–Ω–∏–∏
    closeButton.TextSize = 32  -- –ù–µ–º–Ω–æ–≥–æ —É–≤–µ–ª–∏—á–∏–≤–∞–µ–º —Ä–∞–∑–º–µ—Ä —Ç–µ–∫—Å—Ç–∞
end)

closeButton.MouseLeave:Connect(function()
    closeButton.BackgroundColor3 = Color3.fromRGB(255, 0, 0)  -- –°—Ç–∞–Ω–¥–∞—Ä—Ç–Ω—ã–π –∫—Ä–∞—Å–Ω—ã–π
    closeButton.TextSize = 30  -- –°–±—Ä–æ—Å —Ä–∞–∑–º–µ—Ä–∞ —Ç–µ–∫—Å—Ç–∞
end)

closeButton.MouseButton1Click:Connect(function()
    menuFrame:TweenPosition(UDim2.new(0.5, 0, 1.5, 0), "Out", "Quad", 0.4, true)  -- –ü–ª–∞–≤–Ω–∞—è –∞–Ω–∏–º–∞—Ü–∏—è –∑–∞–∫—Ä—ã—Ç–∏—è
    wait(0.4)
    screenGui:Destroy()
end)

-- –§—É–Ω–∫—Ü–∏—è –¥–ª—è —Å–æ–∑–¥–∞–Ω–∏—è —Ç–µ–∫—Å—Ç–æ–≤–æ–≥–æ –ø–æ–ª—è —Å –∑–∞–∫—Ä—É–≥–ª–µ–Ω–Ω—ã–º–∏ —É–≥–ª–∞–º–∏
local function createTextBox(placeholder, position)
    local textBox = Instance.new("TextBox")
    textBox.Size = UDim2.new(0.8, 0, 0.1, 0)
    textBox.Position = position
    textBox.PlaceholderText = placeholder
    textBox.TextColor3 = Color3.fromRGB(255, 255, 255)
    textBox.BackgroundColor3 = Color3.fromRGB(35, 35, 35)
    textBox.Font = Enum.Font.Gotham
    textBox.TextSize = 20
    textBox.BorderSizePixel = 2
    textBox.BorderColor3 = Color3.fromRGB(0, 255, 255)
    textBox.ClipsDescendants = true
    textBox.Parent = menuFrame
    
    -- –î–æ–±–∞–≤–ª–µ–Ω–∏–µ –∑–∞–∫—Ä—É–≥–ª–µ–Ω–Ω—ã—Ö —É–≥–ª–æ–≤
    local corner = Instance.new("UICorner")
    corner.CornerRadius = UDim.new(0, 10)
    corner.Parent = textBox

    return textBox
end

local recipientTextBox = createTextBox("Recipient's Name", UDim2.new(0.1, 0, 0.1, 0))
local senderTextBox = createTextBox("Sender's Name", UDim2.new(0.1, 0, 0.22, 0))
local quantityTextBox = createTextBox("Quantity", UDim2.new(0.1, 0, 0.34, 0))

-- –§—É–Ω–∫—Ü–∏—è –¥–ª—è —Å–æ–∑–¥–∞–Ω–∏—è –∫–Ω–æ–ø–æ–∫ lootbox —Å –∞–Ω–∏–º–∞—Ü–∏–µ–π –∏ —ç—Ñ—Ñ–µ–∫—Ç–∞–º–∏
