MainSection:NewButton("üèÜ–ü–æ–ª—É—á–∏—Ç—å –ü–æ–±–µ–¥—ãüèÜ", "50 –º–ª–Ω", function()
    game:GetService("ReplicatedStorage"):WaitForChild("Remote"):WaitForChild("Event"):WaitForChild("SurpriseGift"):WaitForChild("[C-S]GetSurpriseGift"):FireServer()
end)

local isToggled = false -- –ü–µ—Ä–µ–º–µ–Ω–Ω–∞—è –¥–ª—è –æ—Ç—Å–ª–µ–∂–∏–≤–∞–Ω–∏—è —Å–æ—Å—Ç–æ—è–Ω–∏—è —Ç—É–º–±–ª–µ—Ä–∞

MainSection:NewToggle("–ü–æ–ª—É—á–∞–π", "–ü–æ–ª—É—á–∞–π", function(state)
    isToggled = state
    if isToggled then
        print("–í–∫–ª")
        spawn(function() -- –°–æ–∑–¥–∞–µ–º –Ω–æ–≤—ã–π –ø–æ—Ç–æ–∫, —á—Ç–æ–±—ã –Ω–µ –±–ª–æ–∫–∏—Ä–æ–≤–∞—Ç—å –æ—Å–Ω–æ–≤–Ω—É—é –ø—Ä–æ–≥—Ä–∞–º–º—É
            while isToggled do
                game:GetService("ReplicatedStorage"):WaitForChild("Remote"):WaitForChild("Event"):WaitForChild("SurpriseGift"):WaitForChild("[C-S]GetSurpriseGift"):FireServer()
                wait(0.01) -- –ó–∞–¥–µ—Ä–∂–∫–∞ –º–µ–∂–¥—É –æ—Ç–ø—Ä–∞–≤–∫–∞–º–∏ (–∏–∑–±–µ–≥–∞–µ—Ç –ø–µ—Ä–µ–≥—Ä—É–∑–∫–∏ —Å–µ—Ä–≤–µ—Ä–∞)
            end
        end)
    else
        print("–í—ã–∫–ª")
    end
end)

local Merga = Window:NewTab("üîÑ–†—É–ª–µ—Ç–∫–∏ (–û—Ç–∫—Ä—ã–≤–∞—Ç—å)üîÑ")
local MergaSection = Merga:NewSection("–û—Ç–∫—Ä—ã–≤–∞–π —É–∂–µ")


MergaSection:NewButton("üîÑüé°–ö—Ä—É—Ç–∏—Ç—å –ö–æ–ª–µ—Å–æüîÑüé°", "–•–∑", function()
    game:GetService("ReplicatedStorage"):WaitForChild("Remote"):WaitForChild("Function"):WaitForChild("Spin"):WaitForChild("[C-S]TrySpin"):InvokeServer()
end)

local isSpinning = false -- –ü–µ—Ä–µ–º–µ–Ω–Ω–∞—è –¥–ª—è –æ—Ç—Å–ª–µ–∂–∏–≤–∞–Ω–∏—è —Å–æ—Å—Ç–æ—è–Ω–∏—è —Ç—É–º–±–ª–µ—Ä–∞

MergaSection:NewToggle("–ù—É, –ö—Ä—É—Ç–∏", "–ö—Ä—É—Ç–∏—Ç—å", function(state)
    isSpinning = state
    if isSpinning then
        print("–í–∫–ª")
        spawn(function()
            while isSpinning do
                game:GetService("ReplicatedStorage"):WaitForChild("Remote"):WaitForChild("Function"):WaitForChild("Spin"):WaitForChild("[C-S]TrySpin"):InvokeServer()
                -- –£–º–µ–Ω—å—à–∞–µ–º –∏–ª–∏ —É–±–∏—Ä–∞–µ–º –∑–∞–¥–µ—Ä–∂–∫—É:
                wait(0.05) -- –ú–∏–Ω–∏–º–∞–ª—å–Ω–∞—è –∑–∞–¥–µ—Ä–∂–∫–∞ –¥–ª—è —Å—Ç–∞–±–∏–ª—å–Ω–æ—Å—Ç–∏
            end
        end)
    else
        print("–í—ã–∫–ª")
    end
end)

local Rebirth = Window:NewTab("üí´–ü–µ—Ä–µ—Ä–æ–∂–¥–µ–Ω–∏—è‚ú®")
local RebirthSection = Rebirth:NewSection("–û—Ç–∫—Ä—ã–≤–∞–π —É–∂–µ")

-- –ü–µ—Ä–µ–º–µ–Ω–Ω—ã–µ –¥–ª—è –æ—Ç—Å–ª–µ–∂–∏–≤–∞–Ω–∏—è —Å–æ—Å—Ç–æ—è–Ω–∏–π
local isRebirthActive = false
local isSuperRebirthActive = false

-- –ö–Ω–æ–ø–∫–∞ –¥–ª—è —Ä—É—á–Ω–æ–≥–æ "–ü–µ—Ä–µ—Ä–æ–∂–¥–µ–Ω–∏—è"
RebirthSection:NewButton("üí´–ü–µ—Ä–µ—Ä–æ–∂–¥–µ–Ω–∏–µ‚ú®", "–•–∑", function()
    game:GetService("ReplicatedStorage"):WaitForChild("Remote"):WaitForChild("Event"):WaitForChild("Eco"):WaitForChild("[C-S]PlayerTryRebirth"):FireServer()
end)

-- –¢—É–º–±–ª–µ—Ä –¥–ª—è –∞–≤—Ç–æ–º–∞—Ç–∏—á–µ—Å–∫–æ–≥–æ "–ü–µ—Ä–µ—Ä–æ–∂–¥–µ–Ω–∏—è"
RebirthSection:NewToggle("–ê–≤—Ç–æ üí´–ü–µ—Ä–µ—Ä–æ–∂–¥–µ–Ω–∏–µ‚ú®", "–í–∫–ª/–í—ã–∫–ª", function(state)
    isRebirthActive = state
    if isRebirthActive then
        print("üí´–ü–µ—Ä–µ—Ä–æ–∂–¥–µ–Ω–∏–µ‚ú® –í–∫–ª")
        spawn(function()
            while isRebirthActive do
                game:GetService("ReplicatedStorage"):WaitForChild("Remote"):WaitForChild("Event"):WaitForChild("Eco"):WaitForChild("[C-S]PlayerTryRebirth"):FireServer()
                wait(0.05) -- –û—á–µ–Ω—å –±—ã—Å—Ç—Ä–∞—è –æ—Ç–ø—Ä–∞–≤–∫–∞
            end
        end)
    else
        print("üí´–ü–µ—Ä–µ—Ä–æ–∂–¥–µ–Ω–∏–µ‚ú® –í—ã–∫–ª")
    end
end)

-- –ö–Ω–æ–ø–∫–∞ –¥–ª—è —Ä—É—á–Ω–æ–≥–æ "–°—É–ø–µ—Ä –ü–µ—Ä–µ—Ä–æ–∂–¥–µ–Ω–∏—è"
RebirthSection:NewButton("–°—É–ø–µ—Ä üí´–ü–µ—Ä–µ—Ä–æ–∂–¥–µ–Ω–∏–µ‚ú®", "–•–∑", function()
    game:GetService("ReplicatedStorage"):WaitForChild("Remote"):WaitForChild("Event"):WaitForChild("Eco"):WaitForChild("[C-S]PlayerTrySuperRebirth"):FireServer()
