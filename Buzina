local Stat = Window:NewTab("Statistics")
local StatSection = Stat:NewSection(".")

-- –§—É–Ω–∫—Ü–∏—è –¥–ª—è —Å–æ–∫—Ä–∞—â–µ–Ω–∏—è —á–∏—Å–µ–ª
local function abbreviateNumber(num)
    if num >= 1e9 then
        return string.format("%.1fb", num / 1e9)
    elseif num >= 1e6 then
        return string.format("%.1fm", num / 1e6)
    elseif num >= 1e3 then
        return string.format("%.1fk", num / 1e3)
    else
        return tostring(num)
    end
end

-- –°–æ–∑–¥–∞–µ–º –ª–µ–π–±–ª –±–µ–∑ –¥–∞–Ω–Ω—ã—Ö
local bestRollLabel = StatSection:NewLabel("Best Roll - –ó–∞–≥—Ä—É–∑–∫–∞...")

-- –ü–æ–ª—É—á–∞–µ–º –∏–≥—Ä–æ–∫–∞
local player = game:GetService("Players").LocalPlayer

-- –ü—Ä–æ–≤–µ—Ä—è–µ–º, —Å—É—â–µ—Å—Ç–≤—É–µ—Ç –ª–∏ leaderstats –∏ Best Roll
if player:FindFirstChild("leaderstats") and player.leaderstats:FindFirstChild("üëë Best Roll") then
    -- –û–±–Ω–æ–≤–ª—è–µ–º –Ω–∞—á–∞–ª—å–Ω–æ–µ –∑–Ω–∞—á–µ–Ω–∏–µ —Å —Å–æ–∫—Ä–∞—â–µ–Ω–∏–µ–º
    bestRollLabel:UpdateLabel("Best Roll - " .. abbreviateNumber(player.leaderstats["üëë Best Roll"].Value))

    -- –î–æ–±–∞–≤–ª—è–µ–º —Å–æ–±—ã—Ç–∏–µ –¥–ª—è –æ–±–Ω–æ–≤–ª–µ–Ω–∏—è –ø—Ä–∏ –∏–∑–º–µ–Ω–µ–Ω–∏–∏
    player.leaderstats["üëë Best Roll"].Changed:Connect(function(newValue)
        bestRollLabel:UpdateLabel("üëëBest Roll - " .. abbreviateNumber(newValue))
    end)
else
    bestRollLabel:UpdateLabel("üëëBest Roll - –¥–∞–Ω–Ω—ã–µ –Ω–µ –Ω–∞–π–¥–µ–Ω—ã")
end


-- –ü–æ–ª—É—á–∞–µ–º –∏–≥—Ä–æ–∫–∞
local player = game:GetService("Players").LocalPlayer

-- –ü—Ä–æ–≤–µ—Ä—è–µ–º, —á—Ç–æ leaderstats —Å—É—â–µ—Å—Ç–≤—É–µ—Ç
if player:FindFirstChild("leaderstats") and player.leaderstats:FindFirstChild("üé≤ Rolls") then
    -- –°–æ–∑–¥–∞–µ–º –ª–µ–π–±–ª –∏ –æ–±–Ω–æ–≤–ª—è–µ–º –µ–≥–æ –∑–Ω–∞—á–µ–Ω–∏–µ
    local rollsLabel = StatSection:NewLabel("üé≤Rolls - " .. player.leaderstats["üé≤ Rolls"].Value)

    -- –û–±–Ω–æ–≤–ª—è–µ–º —Ç–µ–∫—Å—Ç –ø—Ä–∏ –∏–∑–º–µ–Ω–µ–Ω–∏–∏ –∑–Ω–∞—á–µ–Ω–∏—è
    player.leaderstats["üé≤ Rolls"].Changed:Connect(function(newValue)
        rollsLabel:UpdateLabel("üé≤Rolls - " .. newValue)
    end)
else
    StatSection:NewLabel("Rolls - –¥–∞–Ω–Ω—ã–µ –Ω–µ –Ω–∞–π–¥–µ–Ω—ã")
end

