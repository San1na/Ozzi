    -- –£—Å—Ç–∞–Ω–∞–≤–ª–∏–≤–∞–µ–º –æ–±—ä–µ–∫—Ç –≤ –Ω—É–∂–Ω—É—é –ø–æ–∑–∏—Ü–∏—é (–º–æ–∂–Ω–æ –∏–∑–º–µ–Ω–∏—Ç—å)
    padGlow.Position = Vector3.new(0, padGlow.Position.Y, 0)  -- –≠—Ç–æ –º–æ–∂–Ω–æ –∏–∑–º–µ–Ω–∏—Ç—å –Ω–∞ –Ω—É–∂–Ω—ã–µ –∫–æ–æ—Ä–¥–∏–Ω–∞—Ç—ã

    -- –£–±–µ–¥–∏–º—Å—è, —á—Ç–æ –æ–±—ä–µ–∫—Ç –∑–∞–∫—Ä–µ–ø–ª–µ–Ω
    padGlow.Anchored = true

    -- –û—Ç–∫–ª—é—á–∞–µ–º –∫–æ–ª–ª–∏–∑–∏—é, –µ—Å–ª–∏ –Ω—É–∂–Ω–æ
    padGlow.CanCollide = false
end

-- –§—É–Ω–∫—Ü–∏—è –¥–ª—è –≤–æ—Å—Å—Ç–∞–Ω–æ–≤–ª–µ–Ω–∏—è –∏—Å—Ö–æ–¥–Ω–æ–≥–æ —Ä–∞–∑–º–µ—Ä–∞ –∏ –ø–æ–∑–∏—Ü–∏–∏ –æ–±—ä–µ–∫—Ç–∞
local function resetObject()
    padGlow.Size = originalSize
    padGlow.Position = originalPosition
end

-- –ü–µ—Ä–µ–∫–ª—é—á–∞—Ç–µ–ª—å –¥–ª—è —É–≤–µ–ª–∏—á–µ–Ω–∏—è –æ–±—ä–µ–∫—Ç–∞ –≤ 100 —Ä–∞–∑
MiningSection:NewToggle("Obsidian Ore", "", function(state)
    if state then
        print(".")
        enlargeByFactorXAndZ(100)  -- –£–≤–µ–ª–∏—á–∏–≤–∞–µ–º –æ–±—ä–µ–∫—Ç –≤ 100 —Ä–∞–∑ –ø–æ –æ—Å—è–º X –∏ Z
    else
        print(".")
        resetObject()  -- –í–æ–∑–≤—Ä–∞—â–∞–µ–º –æ–±—ä–µ–∫—Ç –∫ –∏—Å—Ö–æ–¥–Ω–æ–º—É —Ä–∞–∑–º–µ—Ä—É –∏ –ø–æ–∑–∏—Ü–∏–∏
    end
end)

local padGlow = workspace.MAP.INTERACT.MiningPads["Runic Ore"].PadGlow

-- –°–æ—Ö—Ä–∞–Ω—è–µ–º –∏—Å—Ö–æ–¥–Ω—ã–µ –∑–Ω–∞—á–µ–Ω–∏—è —Ä–∞–∑–º–µ—Ä–∞ –∏ –ø–æ–∑–∏—Ü–∏–∏ –æ–±—ä–µ–∫—Ç–∞
local originalSize = padGlow.Size
local originalPosition = padGlow.Position

-- –§—É–Ω–∫—Ü–∏—è –¥–ª—è —É–≤–µ–ª–∏—á–µ–Ω–∏—è –æ–±—ä–µ–∫—Ç–∞ –≤ 100 —Ä–∞–∑ –ø–æ –æ—Å—è–º X –∏ Z, –Ω–æ –æ—Å—Ç–∞–≤–ª—è–µ–º –æ—Å—å Y –Ω–µ–∏–∑–º–µ–Ω–Ω–æ–π
local function enlargeByFactorXAndZ(factor)
    -- –£–≤–µ–ª–∏—á–∏–≤–∞–µ–º —Ç–æ–ª—å–∫–æ –æ—Å–∏ X –∏ Z, –æ—Å—å Y –æ—Å—Ç–∞–µ—Ç—Å—è –ø—Ä–µ–∂–Ω–µ–π
    padGlow.Size = Vector3.new(padGlow.Size.X * factor, padGlow.Size.Y, padGlow.Size.Z * factor)
    
    -- –£—Å—Ç–∞–Ω–∞–≤–ª–∏–≤–∞–µ–º –æ–±—ä–µ–∫—Ç –≤ –Ω—É–∂–Ω—É—é –ø–æ–∑–∏—Ü–∏—é (–º–æ–∂–Ω–æ –∏–∑–º–µ–Ω–∏—Ç—å)
    padGlow.Position = Vector3.new(0, padGlow.Position.Y, 0)  -- –≠—Ç–æ –º–æ–∂–Ω–æ –∏–∑–º–µ–Ω–∏—Ç—å –Ω–∞ –Ω—É–∂–Ω—ã–µ –∫–æ–æ—Ä–¥–∏–Ω–∞—Ç—ã

    -- –£–±–µ–¥–∏–º—Å—è, —á—Ç–æ –æ–±—ä–µ–∫—Ç –∑–∞–∫—Ä–µ–ø–ª–µ–Ω
    padGlow.Anchored = true

    -- –û—Ç–∫–ª—é—á–∞–µ–º –∫–æ–ª–ª–∏–∑–∏—é, –µ—Å–ª–∏ –Ω—É–∂–Ω–æ
    padGlow.CanCollide = false
end

-- –§—É–Ω–∫—Ü–∏—è –¥–ª—è –≤–æ—Å—Å—Ç–∞–Ω–æ–≤–ª–µ–Ω–∏—è –∏—Å—Ö–æ–¥–Ω–æ–≥–æ —Ä–∞–∑–º–µ—Ä–∞ –∏ –ø–æ–∑–∏—Ü–∏–∏ –æ–±—ä–µ–∫—Ç–∞
local function resetObject()
    padGlow.Size = originalSize
    padGlow.Position = originalPosition
end

-- –ü–µ—Ä–µ–∫–ª—é—á–∞—Ç–µ–ª—å –¥–ª—è —É–≤–µ–ª–∏—á–µ–Ω–∏—è –æ–±—ä–µ–∫—Ç–∞ –≤ 100 —Ä–∞–∑
MiningSection:NewToggle("Runic Ore", "", function(state)
    if state then
        print(".")
        enlargeByFactorXAndZ(100)  -- –£–≤–µ–ª–∏—á–∏–≤–∞–µ–º –æ–±—ä–µ–∫—Ç –≤ 100 —Ä–∞–∑ –ø–æ –æ—Å—è–º X –∏ Z
    else
        print(".")
        resetObject()  -- –í–æ–∑–≤—Ä–∞—â–∞–µ–º –æ–±—ä–µ–∫—Ç –∫ –∏—Å—Ö–æ–¥–Ω–æ–º—É —Ä–∞–∑–º–µ—Ä—É –∏ –ø–æ–∑–∏—Ü–∏–∏
    end
end)

local isAutoMerchantEnabled = false

-- –ü–µ—Ä–µ–º–µ–Ω–Ω–∞—è –¥–ª—è –æ—Ç—Å–ª–µ–∂–∏–≤–∞–Ω–∏—è —Å–æ—Å—Ç–æ—è–Ω–∏—è
local isAutoMerchantEnabled = false

-- –§—É–Ω–∫—Ü–∏—è –¥–ª—è –≤—ã–ø–æ–ª–Ω–µ–Ω–∏—è –ø–æ–∫—É–ø–∫–∏
local function performPurchase()
    while isAutoMerchantEnabled do
        -- –ü—Ä–æ–≤–µ—Ä—è–µ–º –Ω–∞–ª–∏—á–∏–µ –≤—Å–µ—Ö –Ω–µ–æ–±—Ö–æ–¥–∏–º—ã—Ö –æ–±—ä–µ–∫—Ç–æ–≤
        local replicatedStorage = game:GetService("ReplicatedStorage")
        local network = replicatedStorage:FindFirstChild("Network")
        if network then
            local customMerchant = network:FindFirstChild("CustomMerchants_Purchase")
            if customMerchant then
                for i = 1, 8 do -- –¶–∏–∫–ª —Å 1 –¥–æ 7
                    local args = {
                        [1] = "MiningMerchant",
                        [2] = i, -- –ò–Ω–¥–µ–∫—Å –ø–æ–∫—É–ø–∫–∏
                    }

                    -- –í—ã–ø–æ–ª–Ω—è–µ–º –ø–æ–∫—É–ø–∫—É —Å –∏—Å–ø–æ–ª—å–∑–æ–≤–∞–Ω–∏–µ–º pcall –¥–ª—è –æ—Ç–ª–æ–≤–∞ –æ—à–∏–±–æ–∫
                    local success, err = pcall(function()
                        customMerchant:InvokeServer(unpack(args))
                    end)

                    if not success then
                        warn("–û—à–∏–±–∫–∞ –ø—Ä–∏ –≤—ã–ø–æ–ª–Ω–µ–Ω–∏–∏ InvokeServer: " .. tostring(err))
                    end

                    -- –ó–∞–¥–µ—Ä–∂–∫–∞ –º–µ–∂–¥—É –ø–æ–∫—É–ø–∫–∞–º–∏
                    wait(0.35) -- –£–º–µ–Ω—å—à–∏–ª –∑–∞–¥–µ—Ä–∂–∫—É –¥–ª—è —É—Å–∫–æ—Ä–µ–Ω–∏—è, –µ—Å–ª–∏ —Å–µ—Ä–≤–µ—Ä –ø–æ–∑–≤–æ–ª—è–µ—Ç
                end
            else
                warn("–ù–µ –Ω–∞–π–¥–µ–Ω –æ–±—ä–µ–∫—Ç CustomMerchants_Purchase")
                break
            end
        else
            warn("–ù–µ –Ω–∞–π–¥–µ–Ω –æ–±—ä–µ–∫—Ç Network –≤ ReplicatedStorage")
            break
        end

        -- –î–æ–ø–æ–ª–Ω–∏—Ç–µ–ª—å–Ω–∞—è –∑–∞–¥–µ—Ä–∂–∫–∞ –º–µ–∂–¥—É —Ü–∏–∫–ª–∞–º–∏ –ø–æ–∫—É–ø–æ–∫ (–µ—Å–ª–∏ –Ω—É–∂–Ω–æ)
        wait(0.35)
    end
end

-- –û–±—Ä–∞–±–æ—Ç—á–∏–∫ –ø–µ—Ä–µ–∫–ª—é—á–∞—Ç–µ–ª—è
MiningSection:NewToggle("Autobuy Mining Merchant", "", function(state)
    isAutoMerchantEnabled = state
    if state then
        -- –ó–∞–ø—É—Å–∫–∞–µ–º –≤—ã–ø–æ–ª–Ω–µ–Ω–∏–µ –ø–æ–∫—É–ø–æ–∫
        performPurchase()
    end
end)

-- Auto Chest
local Open = Window:NewTab("Auto Open Mining Chest")
local OpenSection = Open:NewSection(".")

local toggled = false

OpenSection:NewToggle("Basic Mining Chest", "", function(state)
    toggled = state

    if toggled then
        task.spawn(function()
            while toggled do
                local ReplicatedStorage = game:GetService("ReplicatedStorage")
                local Save = require(ReplicatedStorage.Library.Client.Save)
                local Inventory = Save.Get()["Inventory"]
                local RNS = ReplicatedStorage:WaitForChild("Network")

                for i, v in pairs(Inventory.Lootbox) do
                    if v.id == "Basic Mining Chest" then
                        local args = {
                            [1] = i,
                            [2] = 50
                        }
                        RNS:WaitForChild("Lootbox: Open"):InvokeServer(unpack(args))
                    end
                end

                task.wait(0) -- –î–æ–±–∞–≤–ª–µ–Ω –∏–Ω—Ç–µ—Ä–≤–∞–ª –¥–ª—è –ø—Ä–µ–¥–æ—Ç–≤—Ä–∞—â–µ–Ω–∏—è –ø–µ—Ä–µ–≥—Ä—É–∑–∫–∏
            end
        end)
    end
end)

