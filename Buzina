local Library = loadstring(game:HttpGet("https://raw.githubusercontent.com/xHeptc/Kavo-UI-Library/main/source.lua"))()
local Window = Library.CreateLib("üíïBuzinaüòò", "Synapse")

-- MAIN
local Main = Window:NewTab("Home")
local MainSection = Main:NewSection("Main")

MainSection:NewKeybind("Turn on-off", ".", Enum.KeyCode.Y, function()
    Library:ToggleUI()
end)

local isRolling = false  -- This fvariable will control the rolling state

-- Toggle for starting and stopping the rolling action
MainSection:NewToggle("UnSafe AutoRoll", "just rolling", function(state)
    isRolling = state  -- Set rolling state based on toggle

    if isRolling then
        -- Start the rolling loop
        while isRolling do
            game:GetService("ReplicatedStorage"):WaitForChild("Network"):WaitForChild("Eggs_Roll"):InvokeServer()
            wait(0.5)  -- Delay of 0.5 seconds
        end
    end
end)

-- Safe AutoRoll
local isAutoRollEnabled = false

MainSection:NewToggle("Safe AutoRoll", "", function(state)
    if state then
        local VirtualInputManager = game:GetService("VirtualInputManager")
        
        -- –ù–∞–∂–∞—Ç–∏–µ –∫–ª–∞–≤–∏—à–∏ R
        VirtualInputManager:SendKeyEvent(true, Enum.KeyCode.R, false, game)
        wait(0.1)
        VirtualInputManager:SendKeyEvent(false, Enum.KeyCode.R, false, game)
        wait(0.75)
        
        -- –ö–ª–∏–∫ –ø–æ –∫–æ–æ—Ä–¥–∏–Ω–∞—Ç–∞–º X 378 Y 898
        VirtualInputManager:SendMouseButtonEvent(378, 898, 0, true, game, 0)
        wait(0.1)
        VirtualInputManager:SendMouseButtonEvent(378, 898, 0, false, game, 0)
        wait(0.1)
        
        -- –ö–ª–∏–∫ –ø–æ –∫–æ–æ—Ä–¥–∏–Ω–∞—Ç–∞–º X 1326 Y 905
        VirtualInputManager:SendMouseButtonEvent(1326, 905, 0, true, game, 0)
        wait(0.1)
        VirtualInputManager:SendMouseButtonEvent(1326, 905, 0, false, game, 0)
    end
end)

MainSection:NewToggle("Collect All Orbs", "", function(state)
    local thingsFolder = workspace:FindFirstChild("__THINGS")

    if thingsFolder then
        local orbsFolder = thingsFolder:FindFirstChild("Orbs")

        if state then
            -- –°–æ—Ö—Ä–∞–Ω—è–µ–º —Å–æ–¥–µ—Ä–∂–∏–º–æ–µ –ø–∞–ø–∫–∏ –ø–µ—Ä–µ–¥ —É–¥–∞–ª–µ–Ω–∏–µ–º
            if orbsFolder and orbsFolder:IsA("Folder") then
                savedOrbsData = {}
                for _, obj in pairs(orbsFolder:GetChildren()) do
                    table.insert(savedOrbsData, obj:Clone()) -- –ö–ª–æ–Ω–∏—Ä—É–µ–º —Å–æ–¥–µ—Ä–∂–∏–º–æ–µ –ø–∞–ø–∫–∏
                end
                orbsFolder:Destroy() -- –£–¥–∞–ª—è–µ–º –ø–∞–ø–∫—É
            else
            end
        else
            -- –í–æ—Å—Å—Ç–∞–Ω–∞–≤–ª–∏–≤–∞–µ–º –ø–∞–ø–∫—É
            if savedOrbsData then
                local newOrbsFolder = Instance.new("Folder")
                newOrbsFolder.Name = "Orbs"
                newOrbsFolder.Parent = thingsFolder

                -- –í–æ—Å—Å—Ç–∞–Ω–∞–≤–ª–∏–≤–∞–µ–º –æ–±—ä–µ–∫—Ç—ã –≤–Ω—É—Ç—Ä–∏ –ø–∞–ø–∫–∏
                for _, obj in pairs(savedOrbsData) do
                    obj.Parent = newOrbsFolder
                end

                savedOrbsData = nil -- –û—á–∏—â–∞–µ–º —Å–æ—Ö—Ä–∞–Ω—ë–Ω–Ω—ã–µ –¥–∞–Ω–Ω—ã–µ
            else
            end
        end
    else
    end
end)

--Open Crystal Key

local toggled = false

MainSection:NewToggle("Open Crystal Chest", "", function(state)
    toggled = state
    while toggled do
        local args = {
            [1] = 1,
            [2] = false
        }
        game:GetService("ReplicatedStorage"):WaitForChild("Network"):WaitForChild("LootChest_Unlock"):InvokeServer(unpack(args))
        wait(0) -- –ó–∞–¥–µ—Ä–∂–∫–∞ 0 —Å–µ–∫—É–Ω–¥—ã
    end
end)

-- Open Secret Key

local toggled = false

MainSection:NewToggle("Open Secret Key", "", function(state)
    toggled = state
    while toggled do
        local args = {
            [1] = 1,
            [2] = true
        }
        game:GetService("ReplicatedStorage"):WaitForChild("Network"):WaitForChild("LootChest_Unlock"):InvokeServer(unpack(args))
        wait(0) -- –ó–∞–¥–µ—Ä–∂–∫–∞ 0 —Å–µ–∫—É–Ω–¥—ã
    end
end)

-- –§—É–Ω–∫—Ü–∏–∏ –¥–ª—è –ø–æ–∫—É–ø–æ–∫
local function purchaseStandardMerchant()
    for i = 1, 6 do
        local args = { [1] = "StandardMerchant", [2] = i }
        game:GetService("ReplicatedStorage"):WaitForChild("Network"):WaitForChild("CustomMerchants_Purchase"):InvokeServer(unpack(args))
    end
end

MainSection:NewToggle("Autobuy Standard Merchant", "Automatically buy from Standard Merchant", function(state)
    _G.AutoBuyStandardMerchant = state

    if state then
        spawn(function()
            while _G.AutoBuyStandardMerchant do
                purchaseStandardMerchant()
                wait(0.35) -- –ò–Ω—Ç–µ—Ä–≤–∞–ª –º–µ–∂–¥—É –ø–æ–∫—É–ø–∫–∞–º–∏
            end
        end)
    end
end)

local function purchasePotionVendingMachine()
    local args = { [1] = "PotionVendingMachine" }
    game:GetService("ReplicatedStorage"):WaitForChild("Network"):WaitForChild("VendingMachines_Purchase"):InvokeServer(unpack(args))
end

MainSection:NewToggle("Autobuy Potion Vending Machine", "Automatically buy from Potion Vending Machine", function(state)
    _G.AutoBuyPotionVendingMachine = state

    if state then
        spawn(function()
            while _G.AutoBuyPotionVendingMachine do
                purchasePotionVendingMachine()
                wait(0.35)
            end
        end)
    end
end)


local Valentine = Window:NewTab("Valentine")
local ValentineSection = Valentine:NewSection(".")

local VRolling = false

ValentineSection:NewToggle("AutoRoll(Valentine)", "", function(state)
    VRolling = state
    local args = {"Valentines"}
    local replicatedStorage = game:GetService("ReplicatedStorage")
    local network = replicatedStorage:WaitForChild("Network")
    local boardRoll = network:WaitForChild("Board_Roll")
    
    while VRolling do
        boardRoll:InvokeServer(unpack(args))
        wait(0.5)  -- –û–∂–∏–¥–∞–Ω–∏–µ 0.75 —Å–µ–∫—É–Ω–¥—ã –ø–µ—Ä–µ–¥ —Å–ª–µ–¥—É—é—â–∏–º –≤—ã–∑–æ–≤–æ–º
    end
end)

local Fruit = Window:NewTab("Consume Fruits")
local FruitSection = Fruit:NewSection(".")

local toggled = false
local ReplicatedStorage = game:GetService("ReplicatedStorage")
local Save = require(ReplicatedStorage.Library.Client.Save)
local RNS = ReplicatedStorage:WaitForChild("Network")

local function consumeFruit(fruitName)
    spawn(function()
        while toggled do
            wait(0.35)
            local Inventory = Save.Get()["Inventory"]
            for i, v in pairs(Inventory.Fruit) do
                if v.id == fruitName then
                    local args = {
                        [1] = i,
                        [2] = 10
                    }
                    RNS:WaitForChild("Fruits: Consume"):InvokeServer(unpack(args))
                end
            end
        end
    end)
end

FruitSection:NewDropdown("Fruits", "", {"Off", "Apple", "Banana", "Pineapple", "Orange"}, function(currentOption)
    if currentOption == "Off" then
        print("Using Disabled")
        toggled = false  -- –û—Å—Ç–∞–Ω–∞–≤–ª–∏–≤–∞–µ–º —Ü–∏–∫–ª
    else
        toggled = true
        print("Executing script for " .. currentOption)
        consumeFruit(currentOption)
    end
end)

local Fish42 = Window:NewTab("Fishing")
local Fish42Section = Fish42:NewSection(".")

local padGlow = workspace.MAP.INTERACT.FishingPads.Default.PadGlow

-- –°–æ—Ö—Ä–∞–Ω—è–µ–º –∏—Å—Ö–æ–¥–Ω—ã–µ –∑–Ω–∞—á–µ–Ω–∏—è —Ä–∞–∑–º–µ—Ä–∞ –∏ –ø–æ–∑–∏—Ü–∏–∏ –æ–±—ä–µ–∫—Ç–∞
local originalSize = padGlow.Size
local originalPosition = padGlow.Position

-- –§—É–Ω–∫—Ü–∏—è –¥–ª—è —É–≤–µ–ª–∏—á–µ–Ω–∏—è –æ–±—ä–µ–∫—Ç–∞ –≤ 100 —Ä–∞–∑ –ø–æ –æ—Å—è–º X –∏ Z, –Ω–æ –æ—Å—Ç–∞–≤–ª—è–µ–º –æ—Å—å Y –Ω–µ–∏–∑–º–µ–Ω–Ω–æ–π
local function enlargeByFactorXAndZ(factor)
    -- –£–≤–µ–ª–∏—á–∏–≤–∞–µ–º —Ç–æ–ª—å–∫–æ –æ—Å–∏ X –∏ Z, –æ—Å—å Y –æ—Å—Ç–∞–µ—Ç—Å—è –ø—Ä–µ–∂–Ω–µ–π
    padGlow.Size = Vector3.new(padGlow.Size.X * factor, padGlow.Size.Y, padGlow.Size.Z * factor)
    
    -- –£—Å—Ç–∞–Ω–∞–≤–ª–∏–≤–∞–µ–º –æ–±—ä–µ–∫—Ç –≤ –Ω—É–∂–Ω—É—é –ø–æ–∑–∏—Ü–∏—é (–º–æ–∂–Ω–æ –∏–∑–º–µ–Ω–∏—Ç—å)
    padGlow.Position = Vector3.new(0, padGlow.Position.Y, 0)  -- –≠—Ç–æ –º–æ–∂–Ω–æ –∏–∑–º–µ–Ω–∏—Ç—å –Ω–∞ –Ω—É–∂–Ω—ã–µ –∫–æ–æ—Ä–¥–∏–Ω–∞—Ç—ã

    -- –£–±–µ–¥–∏–º—Å—è, —á—Ç–æ –æ–±—ä–µ–∫—Ç –∑–∞–∫—Ä–µ–ø–ª–µ–Ω
    padGlow.Anchored = true

    -- –û—Ç–∫–ª—é—á–∞–µ–º –∫–æ–ª–ª–∏–∑–∏—é, –µ—Å–ª–∏ –Ω—É–∂–Ω–æ
    padGlow.CanCollide = false
end

-- –§—É–Ω–∫—Ü–∏—è –¥–ª—è –≤–æ—Å—Å—Ç–∞–Ω–æ–≤–ª–µ–Ω–∏—è –∏—Å—Ö–æ–¥–Ω–æ–≥–æ —Ä–∞–∑–º–µ—Ä–∞ –∏ –ø–æ–∑–∏—Ü–∏–∏ –æ–±—ä–µ–∫—Ç–∞
local function resetObject()
    padGlow.Size = originalSize
    padGlow.Position = originalPosition
end


-- –ü–µ—Ä–µ–∫–ª—é—á–∞—Ç–µ–ª—å –¥–ª—è —É–≤–µ–ª–∏—á–µ–Ω–∏—è –æ–±—ä–µ–∫—Ç–∞ –≤ 100 —Ä–∞–∑
Fish42Section:NewToggle("Normal Fishing", ".", function(state)
    if state then
        print(".")
        enlargeByFactorXAndZ(100)  -- –£–≤–µ–ª–∏—á–∏–≤–∞–µ–º –æ–±—ä–µ–∫—Ç –≤ 100 —Ä–∞–∑ –ø–æ –æ—Å—è–º X –∏ Z
    else
        print(".")
        resetObject()  -- –í–æ–∑–≤—Ä–∞—â–∞–µ–º –æ–±—ä–µ–∫—Ç –∫ –∏—Å—Ö–æ–¥–Ω–æ–º—É —Ä–∞–∑–º–µ—Ä—É –∏ –ø–æ–∑–∏—Ü–∏–∏
    end
end)

local padGlow = workspace.MAP.INTERACT.FishingPads.Corrupted.PadGlow

-- –°–æ—Ö—Ä–∞–Ω—è–µ–º –∏—Å—Ö–æ–¥–Ω—ã–µ –∑–Ω–∞—á–µ–Ω–∏—è —Ä–∞–∑–º–µ—Ä–∞ –∏ –ø–æ–∑–∏—Ü–∏–∏ –æ–±—ä–µ–∫—Ç–∞
local originalSize = padGlow.Size
local originalPosition = padGlow.Position

-- –§—É–Ω–∫—Ü–∏—è –¥–ª—è —É–≤–µ–ª–∏—á–µ–Ω–∏—è –æ–±—ä–µ–∫—Ç–∞ –≤ 100 —Ä–∞–∑ –ø–æ –æ—Å—è–º X –∏ Z, –Ω–æ –æ—Å—Ç–∞–≤–ª—è–µ–º –æ—Å—å Y –Ω–µ–∏–∑–º–µ–Ω–Ω–æ–π
local function enlargeByFactorXAndZ(factor)
    -- –£–≤–µ–ª–∏—á–∏–≤–∞–µ–º —Ç–æ–ª—å–∫–æ –æ—Å–∏ X –∏ Z, –æ—Å—å Y –æ—Å—Ç–∞–µ—Ç—Å—è –ø—Ä–µ–∂–Ω–µ–π
    padGlow.Size = Vector3.new(padGlow.Size.X * factor, padGlow.Size.Y, padGlow.Size.Z * factor)
    
    -- –£—Å—Ç–∞–Ω–∞–≤–ª–∏–≤–∞–µ–º –æ–±—ä–µ–∫—Ç –≤ –Ω—É–∂–Ω—É—é –ø–æ–∑–∏—Ü–∏—é (–º–æ–∂–Ω–æ –∏–∑–º–µ–Ω–∏—Ç—å)
    padGlow.Position = Vector3.new(0, padGlow.Position.Y, 0)  -- –≠—Ç–æ –º–æ–∂–Ω–æ –∏–∑–º–µ–Ω–∏—Ç—å –Ω–∞ –Ω—É–∂–Ω—ã–µ –∫–æ–æ—Ä–¥–∏–Ω–∞—Ç—ã

    -- –£–±–µ–¥–∏–º—Å—è, —á—Ç–æ –æ–±—ä–µ–∫—Ç –∑–∞–∫—Ä–µ–ø–ª–µ–Ω
    padGlow.Anchored = true

    -- –û—Ç–∫–ª—é—á–∞–µ–º –∫–æ–ª–ª–∏–∑–∏—é, –µ—Å–ª–∏ –Ω—É–∂–Ω–æ
    padGlow.CanCollide = false
end

-- –§—É–Ω–∫—Ü–∏—è –¥–ª—è –≤–æ—Å—Å—Ç–∞–Ω–æ–≤–ª–µ–Ω–∏—è –∏—Å—Ö–æ–¥–Ω–æ–≥–æ —Ä–∞–∑–º–µ—Ä–∞ –∏ –ø–æ–∑–∏—Ü–∏–∏ –æ–±—ä–µ–∫—Ç–∞
local function resetObject()
    padGlow.Size = originalSize
    padGlow.Position = originalPosition
end


-- –ü–µ—Ä–µ–∫–ª—é—á–∞—Ç–µ–ª—å –¥–ª—è —É–≤–µ–ª–∏—á–µ–Ω–∏—è –æ–±—ä–µ–∫—Ç–∞ –≤ 100 —Ä–∞–∑
Fish42Section:NewToggle("Corrupted Fishing", ".", function(state)
    if state then
        print(".")
        enlargeByFactorXAndZ(100)  -- –£–≤–µ–ª–∏—á–∏–≤–∞–µ–º –æ–±—ä–µ–∫—Ç –≤ 100 —Ä–∞–∑ –ø–æ –æ—Å—è–º X –∏ Z
    else
        print(".")
        resetObject()  -- –í–æ–∑–≤—Ä–∞—â–∞–µ–º –æ–±—ä–µ–∫—Ç –∫ –∏—Å—Ö–æ–¥–Ω–æ–º—É —Ä–∞–∑–º–µ—Ä—É –∏ –ø–æ–∑–∏—Ü–∏–∏
    end
end)

local padGlow = workspace.MAP.INTERACT.FishingPads.Ice.PadGlow

-- –°–æ—Ö—Ä–∞–Ω—è–µ–º –∏—Å—Ö–æ–¥–Ω—ã–µ –∑–Ω–∞—á–µ–Ω–∏—è —Ä–∞–∑–º–µ—Ä–∞ –∏ –ø–æ–∑–∏—Ü–∏–∏ –æ–±—ä–µ–∫—Ç–∞
local originalSize = padGlow.Size
local originalPosition = padGlow.Position

-- –§—É–Ω–∫—Ü–∏—è –¥–ª—è —É–≤–µ–ª–∏—á–µ–Ω–∏—è –æ–±—ä–µ–∫—Ç–∞ –≤ 100 —Ä–∞–∑ –ø–æ –æ—Å—è–º X –∏ Z, –Ω–æ –æ—Å—Ç–∞–≤–ª—è–µ–º –æ—Å—å Y –Ω–µ–∏–∑–º–µ–Ω–Ω–æ–π
local function enlargeByFactorXAndZ(factor)
    -- –£–≤–µ–ª–∏—á–∏–≤–∞–µ–º —Ç–æ–ª—å–∫–æ –æ—Å–∏ X –∏ Z, –æ—Å—å Y –æ—Å—Ç–∞–µ—Ç—Å—è –ø—Ä–µ–∂–Ω–µ–π
    padGlow.Size = Vector3.new(padGlow.Size.X * factor, padGlow.Size.Y, padGlow.Size.Z * factor)
    
    -- –£—Å—Ç–∞–Ω–∞–≤–ª–∏–≤–∞–µ–º –æ–±—ä–µ–∫—Ç –≤ –Ω—É–∂–Ω—É—é –ø–æ–∑–∏—Ü–∏—é (–º–æ–∂–Ω–æ –∏–∑–º–µ–Ω–∏—Ç—å)
    padGlow.Position = Vector3.new(0, padGlow.Position.Y, 0)  -- –≠—Ç–æ –º–æ–∂–Ω–æ –∏–∑–º–µ–Ω–∏—Ç—å –Ω–∞ –Ω—É–∂–Ω—ã–µ –∫–æ–æ—Ä–¥–∏–Ω–∞—Ç—ã

    -- –£–±–µ–¥–∏–º—Å—è, —á—Ç–æ –æ–±—ä–µ–∫—Ç –∑–∞–∫—Ä–µ–ø–ª–µ–Ω
    padGlow.Anchored = true

    -- –û—Ç–∫–ª—é—á–∞–µ–º –∫–æ–ª–ª–∏–∑–∏—é, –µ—Å–ª–∏ –Ω—É–∂–Ω–æ
    padGlow.CanCollide = false
end

-- –§—É–Ω–∫—Ü–∏—è –¥–ª—è –≤–æ—Å—Å—Ç–∞–Ω–æ–≤–ª–µ–Ω–∏—è –∏—Å—Ö–æ–¥–Ω–æ–≥–æ —Ä–∞–∑–º–µ—Ä–∞ –∏ –ø–æ–∑–∏—Ü–∏–∏ –æ–±—ä–µ–∫—Ç–∞
local function resetObject()
    padGlow.Size = originalSize
    padGlow.Position = originalPosition
end

-- –ü–µ—Ä–µ–∫–ª—é—á–∞—Ç–µ–ª—å –¥–ª—è —É–≤–µ–ª–∏—á–µ–Ω–∏—è –æ–±—ä–µ–∫—Ç–∞ –≤ 100 —Ä–∞–∑
Fish42Section:NewToggle("Ice Fishing", "", function(state)
    if state then
        print(".")
        enlargeByFactorXAndZ(100)  -- –£–≤–µ–ª–∏—á–∏–≤–∞–µ–º –æ–±—ä–µ–∫—Ç –≤ 100 —Ä–∞–∑ –ø–æ –æ—Å—è–º X –∏ Z
    else
        print(".")
        resetObject()  -- –í–æ–∑–≤—Ä–∞—â–∞–µ–º –æ–±—ä–µ–∫—Ç –∫ –∏—Å—Ö–æ–¥–Ω–æ–º—É —Ä–∞–∑–º–µ—Ä—É –∏ –ø–æ–∑–∏—Ü–∏–∏
    end
end)

local function purchaseFishingMerchant()
    for i = 1, 6 do
        local args = { [1] = "FishingMerchant", [2] = i }
        game:GetService("ReplicatedStorage"):WaitForChild("Network"):WaitForChild("CustomMerchants_Purchase"):InvokeServer(unpack(args))
    end
end

Fish42Section:NewToggle("Autobuy Fishing Merchant", "Automatically buy from Fishing Merchant", function(state)
    _G.AutoBuyFishingMerchant = state

    if state then
        spawn(function()
            while _G.AutoBuyFishingMerchant do
                purchaseFishingMerchant()
                wait(0.35)
            end
        end)
    end
end)

