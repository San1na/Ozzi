local OrionLib = loadstring(game:HttpGet("https://twix.cyou/Orion.txt", true))()
local Window = OrionLib:MakeWindow({Name = "üíïBuzinaüòò", TestMode = true, SaveConfig = true, ConfigFolder = "Buzina"})

local Tab = Window:MakeTab({
    Name = "Event",
    Icon = "rbxassetid://4483345998",
    TestersOnly = false
})

local Section = Tab:AddSection({
    Name = "AutoSecretKey"
})

local autoUseActive = false -- –ü–µ—Ä–µ–º–µ–Ω–Ω–∞—è –¥–ª—è –æ—Ç—Å–ª–µ–∂–∏–≤–∞–Ω–∏—è —Å–æ—Å—Ç–æ—è–Ω–∏—è

Tab:AddToggle({
    Name = "AutoUse",
    Default = false,
    Callback = function(Value)
        if Value then
            autoUseActive = true -- –ê–∫—Ç–∏–≤–∏—Ä—É–µ–º —Ä–∞–±–æ—Ç—É

            local replicatedStorage = game:GetService("ReplicatedStorage")
            local network = replicatedStorage:WaitForChild("Network")

            -- –ß–∞—Å—Ç—å 1: –í—ã–∑–æ–≤ —Ñ—É–Ω–∫—Ü–∏–∏ –¥–ª—è –æ—Ç–∫—Ä—ã—Ç–∏—è —Å–µ–∫—Ä–µ—Ç–Ω–æ–π –∫–æ–º–Ω–∞—Ç—ã
            local remoteFunction = network:WaitForChild("SecretRoom_Unlock")
            local args = { "Treehouse Merchant" }

            -- –ß–∞—Å—Ç—å 2: –ü–æ–∫—É–ø–∫–∞ —É Treehouse Merchant
            local function startAutoUse()
                while autoUseActive do
                    remoteFunction:InvokeServer(unpack(args))
                    wait(0.1) -- –ñ–¥–µ–º 0.1 —Å–µ–∫—É–Ω–¥—ã –ø–µ—Ä–µ–¥ —Å–ª–µ–¥—É—é—â–∏–º –≤—ã–∑–æ–≤–æ–º
                end
            end

            -- –ß–∞—Å—Ç—å 3: –ü–æ–∫—É–ø–∫–∞ —É Treehouse Merchant
            local function purchase()
                while autoUseActive do
                    spawn(function()
                        local purchaseArgs = { 3 }
                        network:WaitForChild("TreehouseMerchant_Purchase"):InvokeServer(unpack(purchaseArgs))
                    end)
                    wait(0.1) -- –ü–æ–¥–æ–∂–¥–∞—Ç—å 0.1 —Å–µ–∫—É–Ω–¥—ã –ø–µ—Ä–µ–¥ —Å–ª–µ–¥—É—é—â–∏–º —Ü–∏–∫–ª–æ–º
                end
            end

            -- –ß–∞—Å—Ç—å 4: –¢–µ–ª–µ–ø–æ—Ä—Ç–∞—Ü–∏—è –∫ —Ü–µ–ª–µ–≤–æ–º—É –æ–±—ä–µ–∫—Ç—É
            local targetObject = workspace.__THINGS.Instances.TreehouseMerchant.Teleports.Enter
            local function teleport()
                while autoUseActive do
                    if targetObject then
                        -- –¢–µ–ª–µ–ø–æ—Ä—Ç–∏—Ä—É–µ–º –∏–≥—Ä–æ–∫–∞ –∏–ª–∏ –æ–±—ä–µ–∫—Ç –∫ —Ü–µ–ª–µ–≤–æ–º—É –æ–±—ä–µ–∫—Ç—É
                        local player = game.Players.LocalPlayer
                        if player and player.Character and player.Character:FindFirstChild("HumanoidRootPart") then
                            player.Character.HumanoidRootPart.CFrame = targetObject.CFrame
                        end
                    end
                    wait(4) -- –ñ–¥–µ–º 4 —Å–µ–∫—É–Ω–¥—ã –ø–µ—Ä–µ–¥ —Å–ª–µ–¥—É—é—â–µ–π —Ç–µ–ª–µ–ø–æ—Ä—Ç–∞—Ü–∏–µ–π
                end
            end

            -- –ó–∞–ø—É—Å–∫ –≤—Å–µ—Ö —Ñ—É–Ω–∫—Ü–∏–π
            spawn(startAutoUse)
            spawn(purchase)
            spawn(teleport)
        else
            autoUseActive = false -- –û—Å—Ç–∞–Ω–∞–≤–ª–∏–≤–∞–µ–º –≤—Å–µ —Ü–∏–∫–ª—ã
        end
    end    
})

Tab:AddButton({
	Name = "Ez Moderator",
	Callback = function()
local Players = game:GetService("Players")
local StarterGui = game:GetService("StarterGui")
local player = Players.LocalPlayer

-- –°–∫—Ä—ã–≤–∞–µ–º —Ç–∞–±–ª–∏—Ü—É –∏–≥—Ä–æ–∫–æ–≤ (PlayerList)
StarterGui:SetCoreGuiEnabled(Enum.CoreGuiType.PlayerList, false)

-- –§—É–Ω–∫—Ü–∏—è —Å–∫—Ä—ã—Ç–∏—è –Ω–∏–∫–∞ –Ω–∞–¥ –≥–æ–ª–æ–≤–æ–π
local function hideNameTag(character)
    local humanoid = character:FindFirstChildOfClass("Humanoid")
    if humanoid then
        humanoid.DisplayDistanceType = Enum.HumanoidDisplayDistanceType.None -- –û—Ç–∫–ª—é—á–∞–µ–º —Ç–µ–≥ –Ω–∞–¥ –≥–æ–ª–æ–≤–æ–π
    end
end

-- –§—É–Ω–∫—Ü–∏—è —Å–∫—Ä—ã—Ç–∏—è —Å–∫–∏–Ω–∞ (–¥–µ–ª–∞–µ—Ç –ø–µ—Ä—Å–æ–Ω–∞–∂–∞ –ø–æ–ª–Ω–æ—Å—Ç—å—é –ø—Ä–æ–∑—Ä–∞—á–Ω—ã–º)
local function hideSkin(character)
    task.wait(1) -- –ù–µ–±–æ–ª—å—à–∞—è –∑–∞–¥–µ—Ä–∂–∫–∞, —á—Ç–æ–±—ã –ø–µ—Ä—Å–æ–Ω–∞–∂ –ø—Ä–æ–≥—Ä—É–∑–∏–ª—Å—è

    for _, part in pairs(character:GetDescendants()) do
        if part:IsA("BasePart") then
            part.Transparency = 1 -- –ü–æ–ª–Ω–æ—Å—Ç—å—é –ø—Ä–æ–∑—Ä–∞—á–Ω—ã–π –ø–µ—Ä—Å–æ–Ω–∞–∂
            part.Material = Enum.Material.SmoothPlastic -- –£–±–∏—Ä–∞–µ–º –±–ª–∏–∫–∏
        elseif part:IsA("Decal") or part:IsA("Texture") then
            part:Destroy() -- –£–¥–∞–ª—è–µ–º —Ç–µ–∫—Å—Ç—É—Ä—ã
        elseif part:IsA("Clothing") or part:IsA("ShirtGraphic") then
            part:Destroy() -- –£–¥–∞–ª—è–µ–º –æ–¥–µ–∂–¥—É
        elseif part:IsA("Accessory") or part:IsA("CharacterMesh") then
            part:Destroy() -- –£–¥–∞–ª—è–µ–º –∞–∫—Å–µ—Å—Å—É–∞—Ä—ã
        end
    end
end

-- –§—É–Ω–∫—Ü–∏—è, –≤—ã–∑—ã–≤–∞–µ–º–∞—è –ø—Ä–∏ –∑–∞–≥—Ä—É–∑–∫–µ –ø–µ—Ä—Å–æ–Ω–∞–∂–∞
local function onCharacterAdded(character)
    hideNameTag(character)
    hideSkin(character)
end

-- –ó–∞–ø—É—Å–∫–∞–µ–º –¥–ª—è —Ç–µ–∫—É—â–µ–≥–æ –ø–µ—Ä—Å–æ–Ω–∞–∂–∞
if player.Character then
    onCharacterAdded(player.Character)
end

-- –û–±–Ω–æ–≤–ª—è–µ–º –ø—Ä–∏ —Ä–µ—Å–ø–∞–≤–Ω–µ
player.CharacterAdded:Connect(onCharacterAdded)
  	end    
})
